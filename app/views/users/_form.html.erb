<div class="text-field">
  <%= form.label :email %>
  <%= form.text_field :email, type: 'email' %>
</div>

<div class="password-field">
  <%= form.label :password %>
  <%= form.password_field :password %>
</div>

<div class="address-field">
  <%= form.label :address %>
  <%= form.text_field :address %>
</div>

<div class="phone-field">
  <%= form.label :phone %>
  <%= form.text_field :phone %>
</div>